{
  "name": "server",
  "version": "2.2.2",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "8.1.1",
    "npm": "^5.0.3"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "test": "mocha",
    "release": "standard-version",
    "major": "npm run release -- --release-as major",
    "minor": "npm run release -- --release-as minor",
    "deploy": "concurrently \"standard-version\" \"npm run push\"",
    "majdeploy": "concurrently \"npm run major\" \"npm run push\"",
    "mindeploy": "concurrently \"npm run minor\" \"npm run push\"",
    "push": "git push -u --follow-tags origin master"
  },
  "author": "Ian Fox Sabey",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "connect-flash": "^0.1.1",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.2",
    "http-errors": "^1.6.2",
    "mocha": "^5.2.0",
    "mongodb": "^3.0.2",
    "mongoose": "^5.0.6",
    "nodemailer": "^4.6.6",
    "nodemon": "^1.14.12",
    "npm": "^6.2.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "standard-version": "^4.4.0",
    "supertest": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eponymz/prerelease-toolkit"
  }
}
